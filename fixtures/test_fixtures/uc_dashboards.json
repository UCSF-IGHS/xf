{
    "model": "uc_dashboards.template",
    "pk": 6,
    "fields": {
        "name": "Home (landing) page",
        "template_type": "1",
        "template_source": "1",
        "template_path": "dashboards/t_home.html",
        "template_text": "{% extends \"t_base.html\" %}\r\n{% load dashgent_filters %}\r\n\r\n{% block main %}\r\n\r\n    <div class=\"page-title\">\r\n        <div class=\"title_left\">\r\n            <h3>Welcome to IBBS\r\n                \r\n            </h3>\r\n        </div>\r\n    </div>\r\n    <div class=\"clearfix\"></div>\r\n    <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n            <div class=\"x_panel\">\r\n                <div class=\"x_title\">\r\n                    <h2>Inbox Design\r\n                        <small>User Mail</small>\r\n                    </h2>\r\n                    <ul class=\"nav navbar-right panel_toolbox\">\r\n                        <li><a class=\"collapse-link\"><i class=\"fa fa-chevron-up\"></i></a>\r\n                        </li>\r\n                        <li class=\"dropdown\">\r\n                            <a href=\"#\" class=\"dropdown-toggle\" data-toggle=\"dropdown\" role=\"button\"\r\n                               aria-expanded=\"false\"><i class=\"fa fa-wrench\"></i></a>\r\n                            <ul class=\"dropdown-menu\" role=\"menu\">\r\n                                <li><a href=\"#\">Settings 1</a>\r\n                                </li>\r\n                                <li><a href=\"#\">Settings 2</a>\r\n                                </li>\r\n                            </ul>\r\n                        </li>\r\n                        <li><a class=\"close-link\"><i class=\"fa fa-close\"></i></a>\r\n                        </li>\r\n                    </ul>\r\n                    <div class=\"clearfix\"></div>\r\n                </div>\r\n                <div class=\"x_content\">\r\n                    <div class=\"row\">\r\n                        <div class=\"col-sm-8 mail_list_column\">\r\n                            <p>{% lorem %}</p>\r\n                            <p>{% lorem %}</p>\r\n                            <p>{% lorem %}</p>\r\n                        </div>\r\n                        <!-- /Welcome panerl-->\r\n\r\n                        <!-- LOGIN MAIL -->\r\n                        <div class=\"col-sm-4 mail_view\">\r\n                        {% if user.is_authenticated %}\r\n\r\n                        {% endif %}\r\n                        {% if not user.is_authenticated %}\r\n                            <div class=\"jubmbotron\">\r\n                                <form id=\"demo-form2\" data-parsley-validate=\"\" class=\"form-horizontal form-label-left\"\r\n                                      novalidate=\"\" name=\"LoginForm\" action=\"\" method=\"post\">\r\n                                    {% csrf_token %}\r\n                                    {% if next %}\r\n                                        <input type=\"hidden\" name=\"next\" value=\"/\"/>\r\n                                    {% endif %}\r\n                                    <p>If you have a login and password for this site, enter it below.</p>\r\n                                    {% if login_incorrect %}\r\n                                        <p>The username and/or password is not correct.</p>\r\n                                    {% endif %}\r\n                                    <div class=\"form-group\">\r\n                                        <label class=\"control-label col-md-4 col-sm-4 col-xs-12\" for=\"username\">User\r\n                                            name<span class=\"required\">*</span>\r\n                                        </label>\r\n                                        <div class=\"col-md-8 col-sm-8 col-xs-12\">\r\n                                            <input type=\"text\" id=\"username\" name=\"username\" required=\"required\"\r\n                                                   class=\"form-control col-md-7 col-xs-12\">\r\n                                        </div>\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <label class=\"control-label col-md-4 col-sm-4 col-xs-12\"\r\n                                               for=\"password\">Password<span class=\"required\">*</span>\r\n                                        </label>\r\n                                        <div class=\"col-md-8 col-sm-8 col-xs-12\">\r\n                                            <input type=\"password\" name=\"password\" id=\"password\" required=\"required\"\r\n                                                   class=\"form-control col-md-7 col-xs-12\">\r\n                                        </div>\r\n                                    </div>\r\n                                    <div class=\"ln_solid\"></div>\r\n                                    <div class=\"form-group\">\r\n                                        <div class=\"col-md-10 col-sm-4 col-xs-12 col-md-offset-4\">\r\n                                            <button type=\"submit\" class=\"btn btn-success btn-sm\">Log in</button>\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                </form>\r\n                            </div>\r\n                            {% endif %}\r\n\r\n                        </div>\r\n                        <!-- /LOGIN MAIL -->\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n{% endblock %}",
        "tags": [
            3
        ]
    }
},
